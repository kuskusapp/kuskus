    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "7.x",
    "@typescript-eslint/parser": "7.x",
    "autoprefixer": "^10.4.19",
    "bun-types": "canary",
    "esbuild-runner": "^2.2.2",
    "eslint": "9.x",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "9.x",
    "inquirer": "^9.2.20",
    "postcss": "^8",
    "prettier": "3.x",
    "tailwindcss": "^3.4.3",
    "typescript": "^5"
  },
  "prettier": {
    "semi": false
