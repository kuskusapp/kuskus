    "@legendapp/state": "^2.2.0-next.75",
    "@sodefa/next-server-context": "^0.0.2",
    "@tailwindcss/forms": "^0.5.7",
    "cloudflare-r2-edge": "^0.0.2",
    "edgedb": "^1.5.4",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.1.0",
    "server-only-context": "^0.1.0",
    "watcher": "^2.3.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@edgedb/generate": "~0.5.3",
    "@eslint/eslintrc": "3.x",
