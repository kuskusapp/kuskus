{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:3000",
    "distDir": "https://kuskus.app",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "KusKus",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "kuskus.app",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/kuskusapp/kuskus/releases/latest/download/latest.json"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDJEQzBDOTAxOTFFMUY0NTIKUldSUzlPR1JBY25BTFd2TE92b292S0l1b3prcWlsRWtPZ2U5cFE0WHVoeGhKMGdzT2ZqNi9ydTUK"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "kuskus",
        "width": 800,
        "height": 600
      }
    ]
  }
}
