{
	"cli": {
		"version": ">= 7.6.2",
		"appVersionSource": "remote",
		"promptToConfigurePushNotifications": false
	},
	"build": {
		"localdev": {
			"developmentClient": true,
			"distribution": "internal",
			"ios": {
				"simulator": true
			},
			"env": {
				"APP_VARIANT": "development"
			}
		},
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "development"
			}
		},
		"preview": {
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "preview"
			},
			"channel": "preview"
		},
		"production": {
			"autoIncrement": true,
			"resourceClass": "large",
			"env": {
				"APP_VARIANT": "production"
			},
			"channel": "production"
		}
	},
	"submit": {
		"production": {}
	}
}
